<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="titleLabel">
            <padding>
                <Insets bottom="5.0" />
            </padding>
        </Label>
        <Label fx:id="priceLabel">
            <padding>
                <Insets bottom="5.0" />
            </padding>
        </Label>
        <Label fx:id="addressLabel">
            <padding>
                <Insets bottom="5.0" />
            </padding>
        </Label>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <padding>
                <Insets bottom="10.0" top="10.0" />
            </padding>
            <children>
                <WebView fx:id="webView" layoutX="210.0" layoutY="56.0" prefHeight="311.0" prefWidth="620.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            </children>
        </AnchorPane>
        <Button maxWidth="Infinity" mnemonicParsing="false" text="Добавить в список покупок" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
